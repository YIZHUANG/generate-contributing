{
  "name": "generate-contributing",
  "description": "Generate a contributing.md, issue_template.md, or pull_request_template.md file for a project. Can be generated from the command line when Generate is installed globally, or as a plugin inside another generator.",
  "version": "0.2.3",
  "homepage": "https://github.com/generate/generate-contributing",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "generate/generate-contributing",
  "bugs": {
    "url": "https://github.com/generate/generate-contributing/issues"
  },
  "license": "MIT",
  "files": [
    "generator.js",
    "index.js",
    "LICENSE",
    "README.md",
    "templates"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "generate-defaults": "^0.6.0",
    "is-valid-app": "^0.2.0"
  },
  "devDependencies": {
    "delete": "^0.3.2",
    "fs-exists-sync": "^0.1.0",
    "generate": "^0.7.3",
    "global-modules": "^0.2.2",
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.3",
    "verb-repo-data": "^0.1.11"
  },
  "keywords": [
    "app",
    "boilerplate",
    "build",
    "cli",
    "cli-app",
    "command-line",
    "contributing",
    "create",
    "dev",
    "development",
    "framework",
    "front",
    "frontend",
    "generate",
    "generate-generator",
    "generate-plugin",
    "generategenerator",
    "generateplugin",
    "generator",
    "init",
    "initialize",
    "new",
    "plugin",
    "project",
    "projects",
    "scaffold",
    "scaffolder",
    "scaffolding",
    "template",
    "templates",
    "tool",
    "web",
    "webapp",
    "yeoman",
    "yo"
  ],
  "lintDeps": {
    "ignore": [
      "scaffolds",
      "templates"
    ]
  },
  "verb": {
    "toc": {
      "method": "preWrite"
    },
    "layout": "generator",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "generate-eslint",
        "generate-license",
        "generate-project",
        "generate-travis"
      ]
    },
    "reflinks": [
      "assemble",
      "base",
      "generate",
      "gulp",
      "verb",
      "verb-generate-readme",
      "verb-readme-generator"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
